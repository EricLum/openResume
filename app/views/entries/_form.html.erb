<% if flash[:error] %>
  <% flash[:error].each do |err| %>
    <%= err %>
  <% end %>
<% end %>

<%= form_for @entry do |f| %>

<%= f.label :job_title %><br>
<%= f.text_field :job_title %><br>

<%= f.label :start_date %><br>
<%= f.date_field :start_date %><br>

<%= f.label :end_date %><br>
<%= f.date_field :end_date %><br>

<%= f.label :description %><br>
<%= f.text_area :description %><br>

<%= f.submit %>
<% end %>
